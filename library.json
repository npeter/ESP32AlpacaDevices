{
    "name": "ESP32AlpacaDevices",
    "version": "0.0.4", 
    "description": "ESP32 based ASCOM Alpaca Server with support of CoverCalibrator-, ObservingCondition- and Switch devices",
    "keywords": "ascom, alpaca, esp32, covercalibrator, observingcondition, switch",
    "repository": {
        "type": "git",
        "url": "https://github.com/npeter/ESP32AlpacaDevices.git"
    },
    "authors": {
        "name": "Peter Neeser",
        "email": "peter_n@gmx.de",
        "maintainer": true
    },
    "license": "MIT",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "headers": [
        "CoverCalibrator.h",
        "ObservingConditions.h",
        "Switch.h"
    ],
    "examples": [
        {
            "name": "Example",
            "base": "examples",
            "files": [
                "main.cpp"
            ]
        }
    ],
    "dependencies": {
        "bblanchon/ArduinoJson": "^7.0.2",
        "https://github.com/mathieucarbou/ESPAsyncWebServer":"*"
    },
    "build": {
        "libCompatMode": "soft"
    }
}